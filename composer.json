{
    "name": "thewebbakery/laravel-custom-cdn",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "TheWebbakery\\CDN\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Noah Wilderom",
            "email": "noah@thewebbakery.nl"
        }
    ],
    "require": {
        "php": ">=8.2",
        "league/flysystem": "^3.7.0",
        "guzzlehttp/guzzle": "^7.8",
        "illuminate/contracts": "^10.39",
        "illuminate/container": "^10.39",
        "illuminate/support": "^10.39",
        "illuminate/http": "^10.39"
    },
    "extra": {
        "laravel": {
            "providers": [
                "TheWebbakery\\CDN\\CDNServiceProvider"
            ]
        }
    }
}
